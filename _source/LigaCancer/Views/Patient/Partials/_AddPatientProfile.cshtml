@model RVCC.Models.FormModel.PatientProfileFormModel

<form id="addPatientProfileForm" asp-action="AddPatientProfile"
        role="form"
        data-ajax="true"
        data-ajax-method="POST"
        data-ajax-failure="error"
        data-ajax-success="addProfileSuccess"
        data-ajax-loading="#submitSpinner">

    <div class="progress m-3 progressAddPatient">
        <div class="progress-bar bg-success" role="progressbar" style="width: 20%;" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100">20%</div>
    </div>

    <partial name="Partials/_PatientProfile" />

</form>
