@model LigaCancer.Models.FormModel.TreatmentPlaceFormModel

<form id="editTreatmentPlaceForm"  asp-action="EditTreatmentPlace"
      role="form"
      data-ajax="true"
      data-ajax-method="POST"
      data-ajax-failure="error"
      data-ajax-success="editSuccess"
      data-ajax-loading="#submitSpinner">

    <div class="modal-body form-horizontal">
        <div class="form-group">
            <label asp-for="City" class="control-label"></label>
            <input asp-for="City" class="form-control" />
            <span asp-validation-for="City" class="text-danger"></span>
        </div>
    </div>

    <partial name="_ModalFooter"  />

</form>
