@model RVCC.Models.FormModel.DoctorFormModel

<form id="addDoctorForm" asp-action="AddDoctor"
      role="form"
      data-ajax="true"
      data-ajax-method="POST"
      data-ajax-failure="error"
      data-ajax-success="addSuccess"
      data-ajax-loading="#submitSpinner">

    <div class="modal-body form-horizontal">
        <partial name="Partials/_FormDoctor" />
    </div>

    <partial name="_ModalFooter"  />

</form>
